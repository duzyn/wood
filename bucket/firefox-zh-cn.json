            "hash": "dc4ce052e44d54aa8402b2b894295755597ca1421cfead974ea0bab16feec4cc"
        },
        "32bit": {
            "url": "https://download.mozilla.org/?product=firefox-78.0.1-SSL&os=win&lang=zh-CN#/firefox.7z",
